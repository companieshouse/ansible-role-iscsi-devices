defaults {
	user_friendly_names no
	find_multipaths yes
}

multipaths {
    {% for device in iscsi_devices_config %}
    multipath {
        wwid    {{ iscsi_devices_vault_config[ansible_hostname]['iscsi_device_wwids'][device.alias] }}
        alias	{{ device.alias }}  
    }
    {% endfor %}
}

