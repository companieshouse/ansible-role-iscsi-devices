{% for device in devices %}
ENV{DM_UUID}=="mpath-{{ iscsi_devices_vault_config[ansible_hostname]['iscsi_device_wwids'][device.alias] }}", RUN+="/bin/raw {{ device.raw_character_device.path }} /dev/mapper/{{ device.alias }}"
KERNEL=="{{ device.raw_character_device.path | basename }}", ACTION=="add", OWNER="{{ device.raw_character_device.user }}", GROUP="{{ device.raw_character_device.group }}", MODE="0660", SYMLINK+="{{ device.alias }}"
{% endfor %}
